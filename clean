#!/bin/csh -f

set nonomatch
rm -f mp.1
rm -rf *.pyc *.pyo .*.sum
rm -rf build .coverage .coverage-html 
rm expected result
foreach i (*/clean)
    if ($i == "*/clean") break
    #echo $i
    (cd $i:h; ./clean)
end
foreach i (*/Makefile)
    if ($i == "*/Makefile") break
    #echo $i
    (cd $i:h; make clean)
end
